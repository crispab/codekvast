- type: file
  path: "/var/log/codekvast/{{ service_name }}/application.log"
  service: "{{ service_name }}"
  source: java
  sourcecategory: codekvast
  log_processing_rules:
      - type: mask_sequences
        name: mask_license_key
        replace_placeholder: "licenseKey=XXXX"
        pattern: "licenseKey=[0-9A-F]+"
