    "systemjs": "0.19.26",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  },
  "scripts": {
    "lite": "lite-server",
    "postinstall": "typings install",
    "posttest": "node_modules/.bin/remap-istanbul -i ../../../build/karma/coverage/coverage-final.json -o ../../../../build/reports/js-coverage -t html",
