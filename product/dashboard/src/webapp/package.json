{
    "name": "codekvast-dashboard",
    "description": "The web interface for Codekvast Dashboard",
    "private": true,
    "sideEffects": false,
    "scripts": {
        "build": "webpack --config config/webpack.prod.js",
        "build:q": "webpack --config config/webpack.dev.js --progress",
        "clean": "rimraf dist; find src -name '*.js' -delete; find src -name '*.js.map' -delete",
        "lint": "tslint ./src/**/*.ts -t verbose",
        "postinstall": "typings install",
        "prebuild": "rimraf dist",
        "start": "webpack-dev-server --progress",
        "test": "karma start && npm run ts-coverage-remap",
        "test:w": "karma start --no-single-run --auto-watch",
        "ts-coverage-remap": "remap-istanbul -i ../../build/reports/frontend-coverage/coverage-final.json -o ../../build/reports/frontend-coverage -t html"
    },
    "license": "MIT",
    "dependencies": {
        "@angular/common": "6.1.10",
        "@angular/compiler": "6.1.10",
        "@angular/core": "6.1.10",
        "@angular/forms": "6.1.10",
        "@angular/http": "6.1.10",
        "@angular/platform-browser": "6.1.10",
        "@angular/platform-browser-dynamic": "6.1.10",
        "@angular/router": "6.1.10",
        "@ng-bootstrap/ng-bootstrap": "3.3.0",
        "core-js": "2.5.7",
        "ngx-cookie": "4.0.2",
        "rxjs": "6.3.3",
        "rxjs-compat": "6.3.3",
        "zone.js": "0.8.26"
    },
    "devDependencies": {
        "@angular/language-service": "6.1.10",
        "@types/google.analytics": "0.0.39",
        "angular2-template-loader": "0.6.2",
        "awesome-typescript-loader": "5.2.1",
        "copy-webpack-plugin": "4.6.0",
        "css-loader": "1.0.1",
        "file-loader": "2.0.0",
        "html-loader": "0.5.5",
        "html-webpack-plugin": "3.2.0",
        "html-webpack-template": "6.2.0",
        "istanbul-instrumenter-loader": "3.0.1",
        "jasmine-core": "3.3.0",
        "karma": "3.1.3",
        "karma-chrome-launcher": "2.2.0",
        "karma-coverage": "1.1.2",
        "karma-firefox-launcher": "1.1.0",
        "karma-jasmine": "2.0.1",
        "karma-junit-reporter": "1.2.0",
        "karma-mocha-reporter": "2.2.5",
        "karma-source-map-support": "1.3.0",
        "karma-sourcemap-loader": "0.3.7",
        "karma-webpack": "3.0.5",
        "mini-css-extract-plugin": "0.4.5",
        "null-loader": "0.1.1",
        "puppeteer": "1.11.0",
        "raw-loader": "0.5.1",
        "reflect-metadata": "0.1.12",
        "remap-istanbul": "0.12.0",
        "rimraf": "2.6.2",
        "rxjs-tslint": "0.1.6",
        "source-map-loader": "0.2.4",
        "style-loader": "0.23.1",
        "tslint": "5.11.0",
        "typescript": "2.8.3",
        "typings": "2.1.1",
        "webpack": "4.27.0",
        "webpack-cli": "3.1.2",
        "webpack-dev-server": "3.1.10",
        "webpack-merge": "4.1.4"
    }
}
