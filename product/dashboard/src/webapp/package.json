{
  "name": "codekvast-dashboard",
  "version": "0.21.6",
  "description": "The web interface for Codekvast Dashboard",
  "private": true,
  "scripts": {
    "build": "webpack --config config/webpack.prod.js --display-optimization-bailout",
    "build:q": "webpack --config config/webpack.dev.js --progress",
    "clean": "rimraf dist; find src -name '*.js' -delete; find src -name '*.js.map' -delete",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "postinstall": "typings install",
    "prebuild": "rimraf dist",
    "start": "webpack-dev-server --inline --progress --port 8088",
    "test": "karma start && npm run ts-coverage-remap",
    "test:w": "karma start --no-single-run --auto-watch",
    "ts-coverage-remap": "remap-istanbul -i ../../build/reports/frontend-coverage/coverage-final.json -o ../../build/reports/frontend-coverage -t html"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "4.4.4",
    "@angular/compiler": "4.4.4",
    "@angular/core": "4.4.4",
    "@angular/forms": "4.4.4",
    "@angular/http": "4.4.4",
    "@angular/platform-browser": "4.4.4",
    "@angular/platform-browser-dynamic": "4.4.4",
    "@angular/router": "4.4.4",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-beta.5",
    "core-js": "2.5.1",
    "intl": "1.2.5",
    "rxjs": "5.4.3",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@types/google.analytics": "^0.0.35",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.2.3",
    "copy-webpack-plugin": "4.1.0",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "1.1.4",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "html-webpack-template": "5.6.0",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine-core": "2.8.0",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-junit-reporter": "1.2.0",
    "karma-mocha-reporter": "2.2.4",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-source-map-support": "1.2.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.4",
    "null-loader": "0.1.1",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.10",
    "remap-istanbul": "0.9.5",
    "rimraf": "2.6.2",
    "source-map-loader": "0.2.2",
    "style-loader": "0.18.2",
    "tslint": "5.7.0",
    "typescript": "2.5.3",
    "typings": "2.1.1",
    "webpack": "3.6.0",
    "webpack-dev-server": "2.9.1",
    "webpack-merge": "4.1.0"
  }
}
