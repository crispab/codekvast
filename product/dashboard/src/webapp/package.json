{
  "name": "codekvast-dashboard",
  "version": "0.22.4",
  "description": "The web interface for Codekvast Dashboard",
  "private": true,
  "scripts": {
    "build": "webpack --config config/webpack.prod.js --display-optimization-bailout",
    "build:q": "webpack --config config/webpack.dev.js --progress",
    "clean": "rimraf dist; find src -name '*.js' -delete; find src -name '*.js.map' -delete",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "postinstall": "typings install",
    "prebuild": "rimraf dist",
    "start": "webpack-dev-server --inline --progress --port 8088",
    "test": "karma start && npm run ts-coverage-remap",
    "test:w": "karma start --no-single-run --auto-watch",
    "ts-coverage-remap": "remap-istanbul -i ../../build/reports/frontend-coverage/coverage-final.json -o ../../build/reports/frontend-coverage -t html"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "5.2.2",
    "@angular/compiler": "5.2.2",
    "@angular/core": "5.2.2",
    "@angular/forms": "5.2.2",
    "@angular/http": "5.2.2",
    "@angular/platform-browser": "5.2.2",
    "@angular/platform-browser-dynamic": "5.2.2",
    "@angular/router": "5.2.2",
    "@ng-bootstrap/ng-bootstrap": "1.0.0",
    "core-js": "2.5.3",
    "rxjs": "5.5.6",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@angular/language-service": "5.2.2",
    "@types/google.analytics": "^0.0.36",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.4.1",
    "copy-webpack-plugin": "4.3.1",
    "css-loader": "0.28.9",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "2.30.1",
    "html-webpack-template": "6.1.0",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine-core": "2.9.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "1.1.1",
    "karma-junit-reporter": "1.2.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-source-map-support": "1.2.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.9",
    "null-loader": "0.1.1",
    "puppeteer": "1.0.0",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.12",
    "remap-istanbul": "0.10.1",
    "rimraf": "2.6.2",
    "source-map-loader": "0.2.3",
    "style-loader": "0.20.1",
    "tslint": "5.9.1",
    "typescript": "2.6.2",
    "typings": "2.1.1",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.1",
    "webpack-merge": "4.1.1"
  }
}
